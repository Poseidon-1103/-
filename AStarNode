public enum E_Node_Type
{
    Walk,
    Stop,
}
public class AStarNode
{
    //格子坐标
    public int x;
    public int y;

    //A*格子类
    //寻路消耗
    public float f;
    //距离起点距离
    public float g;
    //终点的距离
    public float h;
    //格子类型
    public E_Node_Type type;
    //上级对象
    public AStarNode father;

    //输出坐标和格子类型
    public AStarNode(int x0, int y0, E_Node_Type type)
    {
        this.x = x0;
        this.y = y0;
        this.type = type;
    }
}